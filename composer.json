{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^7.1.3 || ^8.0",
    "codeception/codeception": "2.5.6",
    "codeception/specify": "*",
    "codeception/verify": "*",
    "symfony/dotenv": "^4.4.25",
    "yiisoft/yii2": "2.0.42.1",
    "yiisoft/yii2-bootstrap": "^2.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require-dev": {
    "yiisoft/yii2-debug": "^2.1"
  }
}
